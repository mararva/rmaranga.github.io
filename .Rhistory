linplot <- function(x, y) {
d <- data.frame(x=x, y=y)
fit <- lm(y ~ x, data=d)
plot(y ~ x, data=d, pch=19)
abline(fit, lwd=2)
invisible(list(model=fit, fitted=predict(fit), data=d))
}
linplot <- function(x, y) {
